    <link href="~/build/css/custom.min.css" rel="stylesheet" />

    <link href="~/build/css/style.css" rel="stylesheet" />
</head>
<body onload="startTime()" class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="\" class="site_title">
                            <span>Firma İsmi</span>
                        </a>
                    </div>
                    <div class="clearfix"></div>

                    <br />

                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            @Html.Partial("_MainMenu")
                        </div>
                    </div>
                </div>
            </div>

            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a href="javascript:;" class="user-profile dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <i class="fa fa-user"></i> @menuItems.UserName
                                    <span class=" fa fa-angle-down"></span>
                                </a>
                                <ul class="dropdown-menu dropdown-usermenu pull-right">
                                    <li><a href="/Account/ChangePassword">Parola Değiştir</a></li>
                                    <li><a href="/Account/Edit">Profili Güncelle</a></li>
                                    <li><a href="/Account/Logout"><i class="fa fa-sign-out pull-right"></i>Çıkış Yap</a></li>
                                </ul>
                            </li>
                            <li style="margin-right: 5px; margin-top: 15px; font-weight: bold;">
                                <span style="font-size: large;" id="currentTime"></span>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
            <div class="right_col" role="main">
                @RenderBody()
            </div>
            <footer>
                <div class="pull-right">
                    &copy; @DateTime.Now.Year
                </div>
                <div class="clearfix"></div>
            </footer>

        </div>
    </div>

